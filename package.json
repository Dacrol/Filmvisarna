        "popper.js": "^1.12.9"
    },
    "private": true,
    "devDependencies": {
        "@types/jquery": "^3.2.18",
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.2.1",
        "babel-preset-env": "^1.6.1",
        "babelify": "^8.0.0",
        "browser-sync": "^2.23.5",
        "browserify": "^15.2.0",
        "concurrently": "^3.5.1",
        "eslint": "^4.15.0",
        "eslint-config-standard": "^11.0.0-beta.0",
